{
  "name": "lgservice",
  "version": "1.0.21",
  "description": "Helloworld service",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "copy": "copyfiles -u 1 ./src/**/*.json dist/",
    "clean": "rimraf dist",
    "start": "npm run build:live",
    "build": "npm run clean && tsc -p . && npm run copy",
    "pub": "npm run clean && tsc -p . && npm publish",
    "build:live": "nodemon --watch ./src/**/*.ts --exec ts-node ./src/index.ts",
    "test": "mocha -r ts-node/register src/**/*.test.ts"
  },
  "author": "cnboker",
  "license": "BSD",
  "dependencies": {
    "@types/fbemitter": "^2.0.32",
    "axios": "^0.19.0",
    "dotenv": "^8.1.0",
    "fbemitter": "^2.1.1",
    "mqtt": "^3.0.0",
    "querystring": "^0.2.1",
    "string-format": "^2.0.0",
    "strongly-typed-events": "^1.7.3",
    "unzip": "^0.1.11"
  },
  "devDependencies": {
    "@types/chai": "^4.2.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.2",
    "chai": "^4.2.0",
    "copyfiles": "^2.1.1",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "rimraf": "^3.0.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  }
