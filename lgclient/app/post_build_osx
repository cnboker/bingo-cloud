#!/bin/bash

echo start post build
ares-package /Users/scott/code/ioliz/lgclient/app/build /Users/scott/code/ioliz/lgclient/service/fileService/dist  /Users/scott/code/ioliz/lgclient/service/bootservice -o /Users/scott/code/ioliz/lgclient/app/build 
# cp ./build/com.lg.app.signage_0.0.1_all.ipk /Volumes/ESD-USB/application/com.lg.app.signage.ipk
