{
  "include": ["src"],
  "compilerOptions": {
    "sourceMap": true,
    "jsx": "react",
    "target": "ES6",
    "allowJs": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noResolve": false,
    "noImplicitAny": false,
    "removeComments": true
  },
  "exclude": ["./node_modules/**/*"]
}
